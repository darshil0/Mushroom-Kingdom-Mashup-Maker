  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mushroom-kingdom-mashup-maker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/mushroom-kingdom-mashup-maker/issues"
